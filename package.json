{
  "version": "0.0.6",
  "description": "An observable for global state that can be subscribed to with react hooks, or callbacks",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/teaguestockwell/observable-slice"
  },
  "bugs": {
    "url": "https://github.com/teaguestockwell/observable-slice/issues"
  },
  "homepage": "https://github.com/teaguestockwell/observable-slice",
  "keywords": [
    "redux",
    "zustand",
    "jotai",
    "rxjs",
    "state",
    "mobx",
    "xstate"
  ],
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "observable-slice",
  "author": "teagueStockwell",
  "module": "dist/observable-slice.esm.js",
  "size-limit": [
    {
      "path": "dist/observable-slice.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/observable-slice.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "husky": "^8.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "immer": "^9.0.14",
    "lodash.debounce": "^4.0.8",
    "type-fest": "^2.13.0"
  }
}
